{
  "firmware_module": [
    {
      "_id": "am2315_1",
      "environment": "environment_1",
      "type": "am2315"
    },
    {
      "_id": "atlas_ec_1",
      "environment": "environment_1",
      "type": "atlas_ec"
    },
    {
      "_id": "atlas_ph_1",
      "environment": "environment_1",
      "type": "atlas_ph"
    },
    {
      "_id": "atlas_rgb_1",
      "environment": "environment_1",
      "type": "atlas_rgb",
      "arguments": [3]
    },
    {
      "_id": "ph_up_pump_1",
      "environment": "environment_1",
      "type": "software_pwm_actuator",
      "mappings": {"cmd": "water_potential_hydrogen_up_cmd"},
      "arguments": [4, false, 10000]
    },
    {
      "_id": "ph_down_pump_1",
      "environment": "environment_1",
      "type": "software_pwm_actuator",
      "mappings": {"cmd": "water_potential_hydrogen_down_cmd"},
      "arguments": [5, false, 10000]
    },
    {
      "_id": "heater_1",
      "environment": "environment_1",
      "type": "pwm_actuator",
      "mappings": {"cmd": "air_temperature_up_cmd"},
      "arguments": [12]
    },
    {
      "_id": "humidifier_1",
      "environment": "environment_1",
      "type": "pwm_actuator",
      "mappings": {"cmd": "air_humidity_up_cmd"},
      "arguments": [11]
    },
    {
      "_id": "dehumidifier_1",
      "environment": "environment_1",
      "type": "pwm_actuator",
      "mappings": {"cmd": "air_humidity_down_cmd"},
      "arguments": [9]
    },
    {
      "_id": "white_light_1",
      "environment": "environment_1",
      "type": "pwm_actuator",
      "mappings": {"cmd": "white_light_cmd"},
      "arguments": [6]
    },
    {
      "_id": "blue_light_1",
      "environment": "environment_1",
      "type": "pwm_actuator",
      "mappings": {"cmd": "blue_light_cmd"},
      "arguments": [7]
    },
    {
      "_id": "red_light_1",
      "environment": "environment_1",
      "type": "pwm_actuator",
      "mappings": {"cmd": "red_light_cmd"},
      "arguments": [8]
    }
  ],
  "software_module": [
    {
      "_id": "ph_control",
      "type": "openag_brain:pid.py",
      "environment": "environment_1",
      "parameters": {
        "Kp": 0.1,
        "Ki": 0,
        "Kd": 0,
        "upper_limit": 0.1,
        "lower_limit": -0.1,
        "deadband_width": 0.01,
        "variable": "water_potential_hydrogen"
      }
    },
    {
      "_id": "camera_1",
      "type": "usb_cam:usb_cam_node",
      "environment": "environment_1",
      "parameters": {
        "image_width": 1280,
        "image_height": 960,
        "pixel_format": "yuyv",
        "framerate": 1,
        "camera_frame_id": "top",
        "camera_name": "top",
        "autofocus": true,
        "sharpness": 100,
        "io_method": "mmap"
      }
    }
  ]
}
